{
  "DaprSidekick": {
    "Sidecar": {
      "mTLS": true,
      "RuntimeDirectory": "dapr",
      "SentryAddress": "localhost:50001"
    },
    "Sentry": {
      "RuntimeDirectory": "sentry",
      "TrustDomain": "cluster.local" // Certificates in /certs folder generated for this domain
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
